  <script>
$(document).ready(function(){
    $("button").click(function(){
        $.ajax({url: 'xml/witsmlData_1sec.xml',/* 
        	dataType: 'xml' */ success: function(result){
           	console.log(result);
           	
           	var s = $(result).find('note').find('to').text();
           	console.log(s);
        	$("#div1").html(s);
            
        }});
    });
});
</script>

